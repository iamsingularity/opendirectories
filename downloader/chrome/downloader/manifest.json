{
  "manifest_version": 2,

  "name": "Downloader",
  "description": "Downloader",
  "version": "1.15",

  "browser_action": {
    "default_icon": "resources/app.png",
    "default_popup": "resources/app.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "http://*/",
    "contextMenus"
  ],
  "icons": {
    "16": "resources/app.png"
  },
  "background": {
    "page": "resources/background.html",
    "persistent": false
  }
}